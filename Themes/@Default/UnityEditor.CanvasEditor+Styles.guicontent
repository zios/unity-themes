[renderCamera]
text = Render Camera
tooltip = The Camera which will render the canvas. This is also the camera used to send events.

[sortingOrder]
text = Sort Order
tooltip = The order in which Screen Space - Overlay canvas will render

[targetDisplay]
text = Target Display
tooltip = Display on which to render the canvas when in overlay mode