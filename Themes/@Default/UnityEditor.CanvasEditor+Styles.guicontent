[eventCamera]
text = Event Camera
tooltip = The Camera which the events are triggered through. This is used to determine clicking and hover positions if the Canvas is in World Space render mode.

[m_SortingLayerStyle]
text = Sorting Layer

[m_SortingOrderStyle]
